{
  "name": "webeoc-xhtml",
  "displayName": "WebEOC xhtml schema",
  "description": "A schema for WebeEOC board code in VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "webeoc",
      "aliases": ["WebEOC xhtml", "webeoc"],
      "extensions": [".html",".xhtml",".webeoc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "webeoc",
      "scopeName": "text.html.basic",
      "path": "./syntaxes/html.tmLanguage.json"
    }]
  }
}
